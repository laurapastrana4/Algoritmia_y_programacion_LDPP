Algoritmo Ejercicio_7_convertidor_metros
	//Entradas
	Escribir "Ingrese la cantidad en metros: "
	Leer metros
	//caje negra
	pulgadas_totales=metros*39.27
	pie_totales=pulgadas_totales/12
	//salidas
	Escribir "la cantidad en metros es igual a: ", pie_totales " pies y ", pulgadas_totales " pulgadas"
	
FinAlgoritmo
